    <div class = "center-align container">
    {{#event}}
      <h2 class = "bold">{{ event_name }}</h2>
        <img class = "responsive-img" src = "http://localhost:3000{{cover_url}}"/>
        <ul class = "infoPane">
          <li><strong>Date:</strong> {{ startDate }} - {{ endDate }}</li>
          <br/>
          <li><strong>Venue:</strong> {{ venue }}</li>
          <br/>
          <li><strong>About:</strong> {{ about }}</li>
        </ul>

      <div class = "container text-center btnPanel">
          <a class="purple darken-4 btn waves-effect waves-light" target = "blank" href="{{facebook}}" role="button"><i class="mdi-action-label"></i>Facebook</a>
          <a class="purple darken-4 btn waves-effect waves-light" target = "blank" href="{{tickets}}" role="button"><i class="mdi-action-question-answer"></i>Tickets</a>

      </div>

    {{/event}}
    </div>
    <div class="container">
      <h3 class = "left-align">Featured Artists</h3>
      <div class="row">
        {{#artists}}
        <div class="col s6 m4 l3">
         <div class="card purple lighten-1">
           <a href="{{artist_link}} target = "blank"">
             <div class="card-image">
               <img src="http://localhost:3000{{artist_img}}">
               <span class="grey darken-4 card-title">{{artist}}</span>
             </div>
           </a>
         </div>
        </div>
        {{/artists}}
      </div>
    </div>
    {{#event}}
    <div class="container">
      <div class="video-container responsive-video">
        <iframe id="ytplayer" type="text/html" src="{{video}}" frameborder="0"/>
      </div>
    </div>
    {{/event}}
    <br>
